# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

source 'https://github.com/CocoaPods/Specs.git'

target 'OpenProject-Swift' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  # 监测帧率
   pod 'GDPerformanceView-Swift', '~> 1.3.2'
  # 响应式编程
   pod 'RxSwift', '~> 4.1.2'
   pod 'RxCocoa', '~> 4.1.2'
  # 各类样式的tabBarController
   pod 'ESTabBarController-swift', '~> 2.6.1'
  # table view 空数据占位
   pod 'EmptyDataSet-Swift', '~> 4.0.3'
   # 网络请求
   pod 'Moya', '~> 11.0.1'
   # JSON
   pod 'SwiftyJSON'

  target 'OpenProject-SwiftTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'OpenProject-SwiftUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
